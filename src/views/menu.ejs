<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body class="bg-white">
    <%- partial('layouts/navbar2') %>


<aside id="logo-sidebar"
	class="fixed left-0 z-40 w-[350px] h-screen pt-20  "
	aria-label="Sidebar">

	<div class="h-full px-3 pb-4 overflow-y-auto ">
		
		</div>
</aside>

<div class="grid grid-cols-2 p-4 justify-between mt-8 md:ml-[350px]">
	<div class="p-4 sm:w-96 lg:w-[32rem] mt-14">
		<hr class="h-px my-2 w-[108%] bg-gray-700 border-0">

		<ul>
			<% realResponse.forEach(function(i) { %>
				<li>
				  <p>Email: <%= i.email %></p>
				  <p>TypeOf: <%= i.typeOf %></p>
				  <p>Count: <%= i.count %></p>
				</li>
			  <% }); %>
		  </ul>
		  <br><br><br>
		
	</div>

		<div class="ml-32">
			<h2 class="text-black text-sm font-bold mt-14 p-4"></h2>

			<hr class="h-px my-3 bg-gray-200 border-0 ">
			<p><a href="" class="text-sm text-blue-400 hover:underline"></a></p>
		</div>
	</div>
</div>
</body>
</html>